{
  "Marathon": {
    "Endpoint": "http://192.168.1.127:5098"
  },

  "Bamboo": {
    "Endpoint": "http://192.168.1.127:8000",
    "Zookeeper": {
      "Host": "192.168.1.127:5092",
      "Path": "/bamboo",
      "ReportingDelay": 5
    }
  },

  "HAProxy": {
    "TemplatePath": "config/haproxy_template.cfg",
    "OutputDir": "/etc/haproxy",
    "ReloadCommand": "haproxy -f /etc/haproxy/haproxy.cfg -p /var/run/haproxy.pid -D -sf $(cat /var/run/haproxy.pid)",
    "ReloadValidationCommand": "haproxy -c -f {{.}}",
    "IP": "http://127.0.0.1",
    "Port": "5004"
  },

  "StatsD": {
    "Enabled": false,
    "Host": "localhost:8125",
    "Prefix": "bamboo-server.development."
   },

  "Application": {
    "Id": ""
  }
}
